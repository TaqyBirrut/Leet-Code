def romawi(s):
  rom = {"I":1, "V":5, "X":10, "L":50, "C":100, "D":500, "M":1000}
  sekarang = 0
  total = 0
  sebelum = 0
  for i in range (len(s)):
      sekarang = rom[s[i]]
      if sekarang > sebelum:
          total = total + sekarang - 2 * sebelum
      else:
          total += sekarang
      sebelum = sekarang
  return total
 
def main():
  s = str(input(""))
  print(romawi(s))
main()
